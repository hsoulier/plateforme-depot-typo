<div class="h-screen w-full flex overflow-hidden">
    <nav class="flex flex-col bg-black text-white w-64 p-6">
        <div>
            <h2 class="mt-4 text-3xl font-semibold capitalize">
                Bonjour Emilie
            </h2>
        </div>

        <ul class="mt-2 text-gray-200">
            <li class="mt-8">
                <a href="/download" class="flex items-center">
                    <svg class="h-6 w-6 dark:text-gray-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
                        fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                        stroke-linejoin="round">
                        <path d="M3 15v4c0 1.1.9 2 2 2h14a2 2 0 0 0 2-2v-4M17 9l-5 5-5-5M12 12.8V2.5" />
                    </svg>
                    <span class="ml-2 text-base">
                        Tout télécharger
                    </span>
                </a>
            </li>
            <li class="mt-8">
                <a class="change-word flex items-center cursor-pointer">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                        stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                        <polyline points="4 7 4 4 20 4 20 7"></polyline>
                        <line x1="9" y1="20" x2="15" y2="20"></line>
                        <line x1="12" y1="4" x2="12" y2="20"></line>
                    </svg>
                    <span class="ml-2 text-base">
                        Changer le mot
                    </span>
                </a>
            </li>
            <li class="mt-8">
                <a href="/dashboard/update-password" class="flex items-center cursor-pointer">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                        stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                        <rect x="3" y="11" width="18" height="11" rx="2" ry="2"></rect>
                        <path d="M7 11V7a5 5 0 0 1 10 0v4"></path>
                    </svg>
                    <span class="ml-2 text-base">
                        Changer de mot de passe
                    </span>
                </a>
            </li>
        </ul>
        <div class="mt-auto">
            <a class="lowercase text-black bg-white border-white border-2 text-xl px-4 py-2 hover:bg-black hover:text-white transition-colors duration-300 ease-in-out"
                href="/disconnect">deconnexion</a>
        </div>
    </nav>
    <main class="flex-1 flex flex-col bg-white dark:bg-gray-700 transition
		duration-500 ease-in-out overflow-y-auto">
        <div class="mx-10 my-2">
            <h2 class="mt-4 mb-8 text-5xl font-bold dark:text-gray-400">Tous les projets <span
                    class="text-3xl text-gray-400 font-light">({{repos.length}})</span></h2>
            <div class="flex flex-wrap gap-8 mb-8">
                {{#each repos}}
                    <div class="relative text-black w-56 h-56 items-stretch">
                        <div
                            class="absolute p-4 top-0 left-0 w-full h-full border-black border-2 bg-black transform translate-x-2 translate-y-2">
                        </div>
                        <div class="flex flex-col relative p-4 bg-white border-black border-2 h-full">
                            <div class="flex w-full justify-between text-gray-600 text-sm font-light">
                                <span class="flex-end">{{ dateRepo }}</span>
                            </div>
                            <div class="flex items-center capitalize text-2xl font-semibold flex-1 flex-wrap">
                                <div>{{nickname}} {{name}}</div>
                                {{#if description}}
                                    <div class="m-2 p-2 cursor-pointer description-repo" data-message={{message}}>
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                            viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path
                                                d="M21 11.5a8.38 8.38 0 0 1-.9 3.8 8.5 8.5 0 0 1-7.6 4.7 8.38 8.38 0 0 1-3.8-.9L3 21l1.9-5.7a8.38 8.38 0 0 1-.9-3.8 8.5 8.5 0 0 1 4.7-7.6 8.38 8.38 0 0 1 3.8-.9h.5a8.48 8.48 0 0 1 8 8v.5z">
                                            </path>
                                        </svg>
                                    </div>
                                {{/if}}

                            </div>
                            <div class="flex justify-between flex-row-reverse">
                                <div class="inline-flex gap-2">
                                    {{#if socialNetwork.twitter}}
                                        <a href="https://twitter.com/{{socialNetwork.twitter}}" target="_blank"
                                            rel="noopener noreferrer">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                viewBox="0 0 24 24" fill="currentColor">
                                                <path
                                                    d="M24 4.37a9.6 9.6 0 0 1-2.83.8 5.04 5.04 0 0 0 2.17-2.8c-.95.58-2 1-3.13 1.22A4.86 4.86 0 0 0 16.61 2a4.99 4.99 0 0 0-4.79 6.2A13.87 13.87 0 0 1 1.67 2.92 5.12 5.12 0 0 0 3.2 9.67a4.82 4.82 0 0 1-2.23-.64v.07c0 2.44 1.7 4.48 3.95 4.95a4.84 4.84 0 0 1-2.22.08c.63 2.01 2.45 3.47 4.6 3.51A9.72 9.72 0 0 1 0 19.74 13.68 13.68 0 0 0 7.55 22c9.06 0 14-7.7 14-14.37v-.65c.96-.71 1.79-1.6 2.45-2.61z" />
                                            </svg>
                                        </a>
                                    {{/if}}
                                    {{#if socialNetwork.instagram}}
                                        <a href="https://instagram.com/{{socialNetwork.instagram}}" target="_blank"
                                            rel="noopener noreferrer">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                viewBox="0 0 24 24" fill="currentColor">
                                                <path
                                                    d="M16.98 0a6.9 6.9 0 0 1 5.08 1.98A6.94 6.94 0 0 1 24 7.02v9.96c0 2.08-.68 3.87-1.98 5.13A7.14 7.14 0 0 1 16.94 24H7.06a7.06 7.06 0 0 1-5.03-1.89A6.96 6.96 0 0 1 0 16.94V7.02C0 2.8 2.8 0 7.02 0h9.96zm.05 2.23H7.06c-1.45 0-2.7.43-3.53 1.25a4.82 4.82 0 0 0-1.3 3.54v9.92c0 1.5.43 2.7 1.3 3.58a5 5 0 0 0 3.53 1.25h9.88a5 5 0 0 0 3.53-1.25 4.73 4.73 0 0 0 1.4-3.54V7.02a5 5 0 0 0-1.3-3.49 4.82 4.82 0 0 0-3.54-1.3zM12 5.76c3.39 0 6.2 2.8 6.2 6.2a6.2 6.2 0 0 1-12.4 0 6.2 6.2 0 0 1 6.2-6.2zm0 2.22a3.99 3.99 0 0 0-3.97 3.97A3.99 3.99 0 0 0 12 15.92a3.99 3.99 0 0 0 3.97-3.97A3.99 3.99 0 0 0 12 7.98zm6.44-3.77a1.4 1.4 0 1 1 0 2.8 1.4 1.4 0 0 1 0-2.8z" />
                                            </svg>
                                        </a>
                                    {{/if}}
                                </div>
                                <div class="inline-flex gap-2">
                                    <div class="relative dl-btn">
                                        <div
                                            class="list-files absolute top-0 left-0 bg-white border-2 border-black p-2 w-56 hidden">
                                            <span
                                                class="text-right inline-block cursor-pointer close-list-files text-2xl ml-auto mr-0 p-2">x</span>
                                            {{#each files as |file|}}
                                                <div class="mt-1">
                                                    <a href="/uploads/current/{{file}}" target="_blank"
                                                        rel="noopener noreferrer">Fichier {{@index}} -
                                                        {{lookup ../exts @index}}</a>
                                                </div>
                                            {{/each}}
                                        </div>
                                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                            viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                            stroke-linecap="round" stroke-linejoin="round" class="cursor-pointer">
                                            <path
                                                d="M3 15v4c0 1.1.9 2 2 2h14a2 2 0 0 0 2-2v-4M17 9l-5 5-5-5M12 12.8V2.5" />
                                        </svg>
                                    </div>
                                    <a href="mailto:{{email}}">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                            viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                            stroke-linecap="round" stroke-linejoin="round">
                                            <path
                                                d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z">
                                            </path>
                                            <polyline points="22,6 12,13 2,6"></polyline>
                                        </svg>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </main>
</div>


<div
    class="form-change-word z-n1 absolute top-1/2 left-1/2 w-full h-screen transform -translate-x-1/2 -translate-y-1/2 flex items-center justify-center">
    <form action="/change-word" method="post" class="relative w-4/5 mx-auto max-w-xl">
        <div
            class="absolute p-8 top-0 left-0 w-full h-full border-black border-4 bg-black transform translate-x-4 translate-y-4">
        </div>
        <div class="container-element relative px-8 py-4 bg-white border-black border-4">
            <div class="flex justify-end">
                <div class="cursor-pointer close block text-2xl ml-auto mr-0 p-2">x</div>
            </div>
        </div>
    </form>
</div>